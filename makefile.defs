default: all # to be defined in children

clean: clean_extra
	rm -rf *~ includes/*~ src/*~ *.o

../shared/%.o: ../shared/src/%.cpp
	$(MAKE) -C ../shared $(subst ../shared/, , $@)

.PHONY: all clean default clean_extra
